{
    "printWidth": 100,
    "tabWidth": 4,
    "useTabs": false,
    "semi": true,
    "jsxSingleQuote": false,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "trailingComma": "es5",
    "bracketSpacing": true,
    "bracketSameLine": false,
    "arrowParens": "always",
    "endOfLine": "crlf",
    "overrides": [
        {
            "files": "*.scss",
            "options": {
                "trailingComma": "none"
            }
        }
    ]
}
